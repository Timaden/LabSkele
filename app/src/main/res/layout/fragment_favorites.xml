<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorGeorgiaSouthernBlue"
    tools:context="com.example.labskeletest.MainActivity">

    <ExpandableListView
        android:id="@+id/listViewFavorites"
        android:layout_width="match_parent"
        android:layout_height="440dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="50dp"
        android:divider="@color/colorPrimaryDark"
        android:dividerHeight="8dp"
        android:focusable="auto"
        android:overScrollFooter="@color/colorPrimaryDark"
        android:textColor="@android:color/background_light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_Search"
        android:background="@color/colorGeorgiaSouthernBlue"/>S
</android.support.constraint.ConstraintLayout>